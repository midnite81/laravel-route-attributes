{
    "name": "spatie/laravel-route-attributes",
    "description": "Auto register routes using PHP attributes",
    "keywords": [
        "spatie",
        "laravel-route-attributes"
    ],
    "homepage": "https://github.com/spatie/laravel-route-attributes",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\RouteAttributes\\": "src",
            "Spatie\\RouteAttributes\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\RouteAttributes\\Tests\\": "tests",
            "MyOrg\\MyPackage\\": "tests/NamespacedClasses"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\RouteAttributes\\RouteAttributesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
